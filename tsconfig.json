{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "sourceMap": true,
        "outDir": "./dist"
    },
    "include": [
        "src/**/*",
        "launchtest.js",
        "deprecated/deprecated-handlers",
        "deprecated"
    ],
    "exclude": ["node_modules", "**/deprecated/**"]
}
